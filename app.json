{
  "expo": {
    "name": "tourism-mobile",
    "slug": "tourism-mobile",
    "version": "1.0.0",

    "scheme": "tourism-mobile",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "~/assets/expo_starter/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true,

      "tsconfigPaths": true
    },
    "newArchEnabled": true,
    "orientation": "portrait",
    "icon": "~/assets/expo_starter/icon.png",

    "userInterfaceStyle": "dark",

    "splash": {
      "image": "~/assets/expo_starter/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "~/assets/expo_starter/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    }
  }
}
